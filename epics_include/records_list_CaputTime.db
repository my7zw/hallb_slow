record(stringin, "epics_time_now") {
  field(DESC,"current time")
  field(INP, "@%F %T")
  field(DTYP, "Soft Timestamp")
  field(SCAN, "1 second")
}

record(stringin,"caput_time_ChamHv"){
  field(DESC,"epics fill time for ChamHv")
  field(DTYP,"Soft Channel")
  field(PINI,"1")
  field(SCAN,"Passive")
  field(INP,"")
  field(FLNK, "t_reset_ChamHv PP NMS")
  field(MPST,"Always")
}

record(stringin,"epics_time_ChamHv"){
  field(DESC,"epics update time for ChamHv")
  field(DTYP, "Soft Timestamp")
  field(SCAN,"Passive")
  field(PINI,"1")
  field(TSEL, "caput_time_ChamHv.TIME CP")
  field(INP, "@%Y_%m_%d %H:%M:%S")
}

record(seq,"t_reset_ChamHv") {
  field(SCAN,"Passive")
  field(DO1, "0") 
  field(LNK1, "t_counter_ChamHv")
}

record(calc,"t_counter_ChamHv"){
  field(SCAN,"1 second")  
  field(INPA,"3600")	
  field(CALC,"VAL>=A?0:VAL+1")  
  field(LOLO,"-2")
  field(LOW,"-1")
  field(HIGH,"60")
  field(HIHI,"120")
  field(HHSV,"2")
  field(HSV,"1")
  field(LLSV,"2")
  field(LSV,"1")
}

